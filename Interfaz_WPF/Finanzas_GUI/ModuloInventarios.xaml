<Window x:Class="Login_WPF.ModuloInventarios"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ModuloInventarios" Height="505" Width="580" HorizontalAlignment="Center" VerticalAlignment="Center" Loaded="Window_Loaded">
    <TabControl Height="474" Name="tabControl1" Width="563">
        <TabItem Header="Crear Articulo" Name="tabItem1">
            <Grid Height="436">
                <Label Content="Código:" Height="28" HorizontalAlignment="Left" Margin="61,29,0,0" Name="label1" VerticalAlignment="Top" />
                <Label Content="Descripción:" Height="28" HorizontalAlignment="Left" Margin="61,63,0,0" Name="label2" VerticalAlignment="Top" />
                <Label Content="Unidad de medida:" Height="28" HorizontalAlignment="Left" Margin="61,150,0,0" Name="label3" VerticalAlignment="Top" />
                <Label Content="Comentarios:" Height="28" HorizontalAlignment="Left" Margin="66,213,0,0" Name="label4" VerticalAlignment="Top" />
                <Label Content="Imagen:" Height="28" HorizontalAlignment="Left" Margin="66,315,0,0" Name="label5" VerticalAlignment="Top" />
                <TextBox Height="25" HorizontalAlignment="Left" Margin="262,32,0,0" Name="textBoxCodigo" VerticalAlignment="Top" Width="230" />
                <TextBox Height="68" HorizontalAlignment="Left" Margin="262,66,0,0" Name="textBoxDescripcion" VerticalAlignment="Top" Width="230" />
                <ComboBox Height="25" HorizontalAlignment="Left" Margin="262,153,0,0" Name="comboBoxUnidMedida" VerticalAlignment="Top" Width="230" />
                <TextBox Height="75" HorizontalAlignment="Left" Margin="262,215,0,0" Name="textBoxComentarios" VerticalAlignment="Top" Width="230" />
                <TextBox Height="25" HorizontalAlignment="Left" Margin="262,320,0,0" Name="textBoxImagen" VerticalAlignment="Top" Width="157" />
                <Button Content="Agregar" Height="23" HorizontalAlignment="Left" Margin="417,385,0,0" Name="buttonCrearArticulo" VerticalAlignment="Top" Width="75" Click="buttonCrearArticulo_Click" />
                <Button Content="Buscar..." Height="25" HorizontalAlignment="Left" Margin="425,318,0,0" Name="buttonBuscar" VerticalAlignment="Top" Width="67" Click="buttonBuscar_Click" />
            </Grid>
        </TabItem>
        <TabItem Header="Crear Bodega" Name="tabItem2">
            <Grid />
        </TabItem>
    </TabControl>
</Window>
